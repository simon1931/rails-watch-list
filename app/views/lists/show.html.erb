<div class="container">
  <h1><%= @list.name %></h1>
  <h5>Bookmarks</h5>
  <% @list.bookmarks.each do |bookmark| %>
    <p>
      <strong><%= bookmark.movie.title %></strong>
      <%= bookmark.comment %>
    </p>
  <% end %>

<%= link_to "Retour aux lists", lists_path, class: "btn btn-danger" %>
<%= link_to "Ajouter Bookmark", new_list_bookmark_path(@list), class: "btn btn-primary" %>
</div>
